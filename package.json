		"@types/node": "^22.7.5",
		"@types/object-path": "^0.11.4",
		"@types/semver": "^7.5.8",
		"@wessberg/prettier-config": "^1.0.0",
		"@wessberg/ts-config": "^5.0.18",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jsdoc": "^50.3.1",
